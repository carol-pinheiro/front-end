<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> 9</title>
</head>
<body>
    <script>
        var num1 = parseFloat(prompt("Digite o primeiro número:"));
        var num2 = parseFloat(prompt("Digite o segundo número :"));
        var num3 = parseFloat(prompt("Digite o terceiro número:"));
        var num4 = parseFloat(prompt("Digite outro número:"));

        var numeros = [num1, num2, num3, num4].sort(function(a, b) {
            return b - a;
        });

        alert("Os números em ordem decrescente são: " + numeros.join(", "));
    </script>
