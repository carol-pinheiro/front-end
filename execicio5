    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>5</title>
</head>
<body>
    <script>
        var num1 = parseFloat(prompt("Digite o primeiro número: "));
        var num2 = parseFloat(prompt("Digite o segundo número: "));

        if (num1 > num2) {
            alert(num1 + " é maior");
        } else {
            alert(num2 + " é maior");
        }
    </script>
